###  https://w3id.org/italia/onto/Transparency/TransparencyActivity
:TransparencyActivity rdf:type owl:Class ;
                      rdfs:subClassOf l0:Activity ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty :generatesResource ;
                                        owl:someValuesFrom :TransparencyResource
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty :hasTransparencyActivityTypology ;
                                        owl:someValuesFrom :TransparencyActivityTypology
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty :isPerformedByAgent ;
                                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                        owl:onClass l0:Agent
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty tiapit:date ;
                                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                        owl:onDataRange [ rdf:type rdfs:Datatype ;
                                                          owl:unionOf ( xsd:date
                                                                        xsd:dateTime
                                                                      )
                                                        ]
                                      ] ;
                      owl:disjointUnionOf ( :TransparencyPublicationActivity
                                            :TransparencyUpdateActivity
                                          ) ;
                      rdfs:comment "Questa classe rappresenta un'attività di trasparenza come conseguenza di un obbligo di trasparenza."@it ,
                                   "This class represents a trasparency activity as an effect of a transparency obbligation. For example pubblication, update,..."@en ;
                      rdfs:isDefinedBy <https://w3id.org/italia/onto/Transparency> ;
                      rdfs:label "Attività di trasparenza."@it ,
                                 "Transparency activity"@en ;
                      owl:versionInfo "stabile"@it ,
                                      "stable"@en .


